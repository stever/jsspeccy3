{
  "AllowedHosts": "*",
  "SAML": {
    "AppId": "zxplay",
    "DefaultExpirationMinutes": 480,
    "AdmitNewUsers": true,
    "AuthCookieName": "access_token",
    "ReturnUrlCookieName": "redirect_url"
  },
  "JWT": {
    "DefaultRole": "zxplay-user",
    "AddDefaultRole": true,
    "SessionToken": {
      "ExpirationSeconds": 28800,
      "Issuer": "zxplay",
      "Audience": "caddy"
    },
    "HasuraToken": {
      "ExpirationSeconds": 900,
      "Issuer": "zxplay",
      "Audience": "hasura"
    }
  }
}